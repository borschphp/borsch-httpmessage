{
  "name": "borschphp/httpmessage",
  "description": "A PSR-7 and PSR-17 implementation.",
  "keywords": [
    "http",
    "message",
    "http-message",
    "psr-7",
    "psr-17",
    "factory"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alexandre DEBUSSCHERE",
      "email": "alexandre.debusschere@hey.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-simplexml": "*",
    "psr/http-message": "^1||^2",
    "psr/http-factory": "^1",
    "ralouphie/getallheaders": "^3.0"
  },
  "require-dev": {
    "pestphp/pest": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "Borsch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BorschTest\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
